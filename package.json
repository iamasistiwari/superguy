{
  "name": "superguy",
  "private": true,
  "scripts": {
    "prettier": "bunx prettier --write .",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,py,md}\"",
    "lint": "turbo run lint",
    "check-types": "turbo run check-types",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.5.1",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.8",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
